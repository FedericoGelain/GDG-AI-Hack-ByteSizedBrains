{
  "name": "Audio Recorder",
  "version": "1.0",
  "description": "Records audio for 5 seconds",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "host_permissions": [
  "http://localhost:5000/*",
  "http://127.0.0.1:5000/*"
],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [{
    "resources": ["recorder.html"],
    "matches": ["<all_urls>"]
  }]
}